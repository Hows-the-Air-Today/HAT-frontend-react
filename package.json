    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.2",
    "ts-jest": "^29.1.0",
    "tsconfig-paths": "^4.2.0",
    "webpack": "^5.78.0"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write"
